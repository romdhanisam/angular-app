{
  "name": "angular-app",
  "version": "0.0.4",
  "license": "MIT license",
  "author": {
    "name": "Samir Romdhani",
    "email": "samir.romdhani1994@gmail.com"
  },
  "homepage": "https://github.com/romdhanisam/angular-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/romdhanisam/angular-app.git"
  },
  "bugs": {
    "url": "https://github.com/romdhanisam/angular-app/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "start:qa": "ng serve --configuration qa",
    "start:prod": "ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --no-cache",
    "test:cov": "jest --no-cache --coverage",
    "test:watch": "jest --no-cache --watch",
    "test:ci": "jest --no-cache --coverage --detectOpenHandles --ci",
    "test:ci:active": "jest --no-cache --coverage --detectOpenHandles --ci --testNamePattern=DevTest",
    "ts:check": "eslint .",
    "html:check": "htmlhint --config .htmlhintrc.json '**/template.html'",
    "html:prettier": "prettier src/**/template.html --check",
    "deploy": "ng deploy --no-notfound --no-nojekyll --dir=dist/",
    "e2e": "ng e2e",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:headed": "cypress run --headed",
    "ci:start-server": "angular-http-server --path ./dist -p 4200",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:4200 cy:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.7",
    "@angular/cdk": "16.2.7",
    "@angular/common": "16.2.7",
    "@angular/compiler": "16.2.7",
    "@angular/core": "16.2.7",
    "@angular/forms": "16.2.7",
    "@angular/localize": "16.2.7",
    "@angular/material": "16.2.7",
    "@angular/platform-browser": "16.2.7",
    "@angular/platform-browser-dynamic": "16.2.7",
    "@angular/router": "16.2.7",
    "@angular/service-worker": "16.2.7",
    "@ngrx/store": "16.0.0",
    "@ngrx/store-devtools": "16.0.0",
    "angular-cli-ghpages": "^2.0.0-beta.2",
    "bootstrap": "5.0.0",
    "font-awesome": "4.7.0",
    "rxjs": "~7.8.0",
    "ts-jest": "^29.1.2",
    "tslib": "2.3.0",
    "w3c-schemas": "1.4.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "16.0.1",
    "@angular-devkit/build-angular": "16.2.7",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.7",
    "@angular/compiler-cli": "16.2.7",
    "@cypress/schematic": "^2.5.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "angular-http-server": "^1.12.0",
    "cypress": "^13.6.3",
    "eslint": "^8.49.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "htmlhint": "^1.1.4",
    "jest": "29.7.0",
    "jest-preset-angular": "^14.0.0",
    "ng-packagr": "16.0.0",
    "prettier": "3.2.5",
    "process": "0.11.10",
    "sass": "1.68.0",
    "sass-loader": "13.3.2",
    "start-server-and-test": "^2.0.3",
    "stream-http": "^3.2.0",
    "typescript": "~5.0.2",
    "uuid": "^8.3.2"
  },
